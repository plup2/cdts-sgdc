/**
 * This is all of the html to be included before existing page content.
 * @param? search Used to display search.
 * @param? siteMenu Used to display site menu.
 * @param? menuPath path for site menu.
 * @param? lngLinks Links to pages in other languages.
 * @param? showPreContent Used to determine if the precontent space will be displayed.
 * @param? breadcrumbs Used to set custom breadcrumbs.
 * @param? topSecMenu Set to true if you want to use the secondary navigation.
 * @param? subTheme Used to identify the custom subTheme.
 * @param? intranetTitle Used to identify the title of the intranet site.
 * @param? cdnEnv Used to switch between qat, nonprod, and prod CDN implementations.
 * @param? GCToolsModal Used to turn the GCTools links into a modal.
 * @param? customSearch Used to control the custom searches.
*/
{template .top}
	<ul id="wb-tphp">
		<li class="wb-slc"><a class="wb-sl" href="#wb-cont">{msg desc=""}Skip to main content{/msg}</a></li>
		<li class="wb-slc visible-xs visible-sm visible-md visible-lg"><a class="wb-sl" href="#wb-info">{msg desc=""}Skip to "About this site"{/msg}</a></li>
		{if $topSecMenu == true}
		<li class="wb-slc visible-md visible-lg"><a class="wb-sl" href="#wb-sec">{msg desc=""}Skip to section menu{/msg}</a></li>
		{/if}
	</ul>
	<header role="banner">
		<div id="wb-bnr">
			<div id="app-brand">
				<div class="container">
					<div class="row">
						<section class="{if (isNonnull($intranetTitle)) or (isNonnull($subTheme))}col-lg-6 col-md-6{else}col-lg-2 col-md-3{/if} col-sm-9 col-xs-8">
							<div class="app-name{if isNonnull($intranetTitle)} app-name-lg{/if}">
								{if isNonnull($intranetTitle)}
									{foreach $item in $intranetTitle}
										{if isFirst($item)}
											<a href="
											{if isNonnull($item.href)}
												{$item.href}
											{else}
												{msg desc=""}http://intranet.ec.gc.ca/default.asp?lang=En{/msg}
											{/if}
											">
											<span>
											{if isNonnull($item.acronym)}
												<abbr title="{$item.acronym}">
											{/if}
											<span class="bold-gc">
											{if isNonnull($item.boldText)}
												{$item.boldText}
											{else}
												{msg desc=""}GC{/msg}
											{/if}
											</span>
											{if isNonnull($item.text)}
												{$item.text} 
											{else}
												{msg desc=""}intranet{/msg}
											{/if}
											{if isNonnull($item.acronym)}
												</abbr>
											{/if}
											</span>
											</a>
										{/if}
									{/foreach}
								{else}
								<a href="{msg desc=""}https://intranet.canada.ca/index-eng.asp{/msg}"><span>{msg desc=""}<span class="bold-gc">GC</span> intranet{/msg}</span></a>
								{/if}
							</div>
						</section>
						{if $GCToolsModal == true}
						<section id="gctoolsSection" class="{if isNonnull($intranetTitle)}col-lg-2 col-md-1{else}col-lg-6 col-md-1{/if}{if $search == false} col-md-offset-3{/if} hidden-sm hidden-xs text-right">
							<h2 class="wb-inv">{msg desc=""}GCTools links{/msg}</h2>
							<div class="row">
								<div class="col-md-12">
									<ul class="list-inline margin-bottom-none gcbarlng">
										<li><a class="wb-lbx" href="#gctools">{msg desc=""}<span class="bold-gc">GC</span>Tools{/msg}</a></li>
									</ul>
								</div>
							</div>
							<section id="gctools" class="mfp-hide modal-dialog modal-content overlay-def">
								<header class="modal-header">
									<h2 class="modal-title">{msg desc=""}<span class="bold-gc">GC</span>Tools{/msg}</h2>
								</header>
								<div class="modal-body">
									<ul class="list-unstyled lst-spcd">
										<li><a href="{msg desc=""}https://gcconnex.gc.ca/splash/{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">{msg desc=""}<span class="bold-gc">GC</span>connex{/msg}</a></li>
										<li><a href="{msg desc=""}http://www.gcpedia.gc.ca/wiki/?setlang=en{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">{msg desc=""}<span class="bold-gc">GC</span>pedia{/msg}</a></li>
										<li><a href="{msg desc=""}http://gcdirectory-gcannuaire.gc.ca/en/GCD/?pgid=002{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">{msg desc=""}<span class="bold-gc">GC</span>directory{/msg}</a></li>
										<li><a href="{msg desc=""}https://gccollab.ca/splash/{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">{msg desc=""}<span class="bold-gc">GC</span>collab{/msg}</a></li>
										<li><a href="{msg desc=""}https://intranet.canada.ca/index-eng.asp{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">{msg desc=""}<span class="bold-gc">GC</span>intranet{/msg}</a></li>
									</ul>
								</div>
							</section>
						</section>
						{elseif isNonnull($intranetTitle)}
						{else}
						<section id="wb-gctools" class="col-lg-6 visible-lg">
							<div class="pull-left tool-link">
								<a href="{msg desc=""}https://gcconnex.gc.ca/splash/{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">
								{msg desc=""}<span class="bold-gc">GC</span>connex{/msg}</a>
							</div>
							<div class="pull-left tool-link">
								<a href="{msg desc=""}http://www.gcpedia.gc.ca/wiki/?setlang=en{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">
								{msg desc=""}<span class="bold-gc">GC</span>pedia{/msg}</a>
							</div>
							<div class="pull-left tool-link">
								<a href="{msg desc=""}http://gcdirectory-gcannuaire.gc.ca/en/GCD/?pgid=002{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">
								{msg desc=""}<span class="bold-gc">GC</span>directory{/msg}</a>
							</div>
							<div class="pull-left tool-link">
								<a href="{msg desc=""}https://gccollab.ca/splash/{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">
								{msg desc=""}<span class="bold-gc">GC</span>collab{/msg}</a>
							</div>
						</section>
						{/if}
						{if isNonnull($lngLinks)}
						<section id="wb-lng" class="{if (isNonnull($intranetTitle) and ($GCToolsModal != true))}mrgn-bttm-md col-md-offset-1 col-lg-offset-2 {/if}col-md-1 hidden-sm hidden-xs text-right {if $search == false}lang-no-search{else}lang-with-search{/if}">
							<h2 class="wb-inv">{msg desc=""}Language selection{/msg}</h2>
							<div class="row">
								<div class="col-md-12">
									<ul class="list-inline margin-bottom-none gcbarlng">
										{foreach $link in $lngLinks}
											<li><a hreflang="{$link.lang}" lang="{$link.lang}" href="{$link.href}">{$link.text}</a></li>
										{/foreach}
									</ul>
								</div>
							</div>
						</section>
						{/if}
						{if $search != false}
							<section id="wb-srch" class="col-lg-3 col-md-4 hidden-sm hidden-xs search-md">
								<h2>{msg desc=""}Search{/msg}</h2>
								{if isNonnull($customSearch)}
								{foreach $item in $customSearch}
									{if isFirst($item)}
									<form action="
									{if isNonnull($item.action)}
										{$item.action}
									{else}
										{msg desc=""}https://intranet.canada.ca/search-recherche/query-recherche-eng.aspx{/msg}
									{/if}
									" method="
									{if isNonnull($item.method)}
										{$item.method}
									{else}
										get
									{/if}
									" name="
									{if isNonnull($item.name)}
										{$item.name}
									{else}
										cse-search-box
									{/if}
									" role="search" class="form-inline">
										<div class="form-group">
											<label for="
											{if isNonnull($item.id)}
												{$item.id}
											{else}
												wb-srch-q
											{/if}
											" class="wb-inv">
											{if isNonnull($item.placeholder)}
												{$item.placeholder}
											{else}
												{msg desc=""}Search GCintranet{/msg}
											{/if}
											</label>
											<input type="text" class="wb-srch-q form-control pull-right" id="
											{if isNonnull($item.id)}
												{$item.id}
											{else}
												wb-srch-q
											{/if}
											" name="
											{if isNonnull($item.name)}
												{$item.name}
											{else}
												q
											{/if}
											"  value="" size="21" maxlength="150" placeholder="
											{if isNonnull($item.placeholder)}
												{$item.placeholder}
											{else}
												{msg desc=""}Search GCintranet{/msg}
											{/if}
											">
											{if isNonnull($item.hiddenInput)}
											{foreach $input in $item.hiddenInput}
												<input type="hidden" name="{$input.name}" value="{$input.value}">
											{/foreach}
											{else}
												<input type="hidden" name="a" value="s">
												<input type="hidden" name="s" value="1">
												<input type="hidden" name="chk2" value="True">
											{/if}
										</div>
										<div class="form-group submit">&nbsp;
											<button type="submit" id="wb-srch-sub" class="btn btn-primary btn-small" name="wb-srch-sub"><span class="glyphicon-search glyphicon"></span><span class="wb-inv">{msg desc=""}Search{/msg}</span></button>
										</div>
									</form>
									{/if}
								{/foreach}
								{else}
									<form action="{msg desc=""}https://intranet.canada.ca/search-recherche/query-recherche-eng.aspx{/msg}" method="get" name="cse-search-box" role="search" class="form-inline">
										<div class="form-group">
											<label for="wb-srch-q" class="wb-inv">{msg desc=""}Search GCintranet{/msg}</label>
											<input type="text" class="wb-srch-q form-control pull-right" id="wb-srch-q" name="q"  value="" size="21" maxlength="150" placeholder="{msg desc=""}Search GCintranet{/msg}">
											<input type="hidden" name="a" value="s">
											<input type="hidden" name="s" value="1">
											<input type="hidden" name="chk2" value="True">
										</div>
										<div class="form-group submit">&nbsp;
											<button type="submit" id="wb-srch-sub" class="btn btn-primary btn-small" name="wb-srch-sub"><span class="glyphicon-search glyphicon"></span><span class="wb-inv">{msg desc=""}Search{/msg}</span></button>
										</div>
									</form>
								{/if}
							</section>
						{/if}
						<section id="wb-glb-mn" class="wb-mb-links col-xs-4 col-sm-3 visible-sm visible-xs">
							<h2>
							{if $search == false}
								{msg desc=""}Menu{/msg}{if isNonnull($topSecMenu)}s{/if}
							{else}
								{msg desc=""}Search and menu{/msg}{if isNonnull($topSecMenu)}s{/if}
							{/if}
							</h2>
							<ul class="list-inline text-right chvrn">
								<li><a href="#mb-pnl" title="
								{if $search == false}
									{msg desc=""}Menu{/msg}{if isNonnull($topSecMenu)}s{/if}
								{else}
									{msg desc=""}Search and menu{/msg}{if isNonnull($topSecMenu)}s{/if}
								{/if}
								" aria-controls="mb-pnl" class="overlay-lnk" role="button">
									{if $search != false}<span class="glyphicon glyphicon-search">{/if}
									<span class="glyphicon glyphicon-th-list">
									<span class="wb-inv">
									{if $search == false}
										{msg desc=""}Menu{/msg}{if isNonnull($topSecMenu)}s{/if}
									{else}
										{msg desc=""}Search and menu{/msg}{if isNonnull($topSecMenu)}s{/if}
									{/if}
									</span>
									</span>
									{if ($search != false)}</span>{/if}
								</a></li>
							</ul>
							<div id="mb-pnl"></div>
						</section>
					</div>
					{if $GCToolsModal != true}
					<div id="tools-sm" class="row{if isNonnull($intranetTitle)}{else} visible-md{/if}">
						<section id="tool-container-sm" class="col-md-12 col-sm-12">
							<div class="pull-left tool-link"><a href="{msg desc=""}https://gcconnex.gc.ca/splash/{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">{msg desc=""}<span class="bold-gc">GC</span>connex{/msg}</a></div>
							<div class="pull-left tool-link"><a href="{msg desc=""}http://www.gcpedia.gc.ca/wiki/?setlang=en{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">{msg desc=""}<span class="bold-gc">GC</span>pedia{/msg}</a></div>
							<div class="pull-left tool-link"><a href="{msg desc=""}http://gcdirectory-gcannuaire.gc.ca/en/GCD/?pgid=002{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">{msg desc=""}<span class="bold-gc">GC</span>directory{/msg}</a></div>
							<div class="pull-left tool-link"><a href="{msg desc=""}https://gccollab.ca/splash/{/msg}" class="{msg desc=""}GCTBannerOnclick-eng{/msg}">{msg desc=""}<span class="bold-gc">GC</span>collab{/msg}</a></div>
						</section>
					</div>
					{/if}
				</div>
			</div>
		</div>
		{if $siteMenu != false}
		{if ($subTheme == 'esdc') or ($subTheme == 'labour')}
		<nav role="navigation" id="wb-sm" data-wb-ajax='{lb}"url": "{if isNonnull($menuPath)}{$menuPath}{else}{msg desc=""}https://esdc.prv/includes/edsc-esdc_prv/wet4/en/mega_menu/esdcmenu-eng.html{/msg}{/if}", "type": "replace", "nocache": true {rb}' data-trgt="mb-pnl" class="wb-menu visible-md visible-lg" typeof="SiteNavigationElement">
			<div class="container nvbar">
				<h2>{msg desc=""}Topics menu{/msg}</h2>
				<div class="row">
					<ul class="list-inline menu">
						<li><a href="{msg desc=""}http://esdc.prv/en/department/index.shtml{/msg}">{msg desc=""}Our Department{/msg}</a></li>
						<li><a href="{msg desc=""}http://esdc.prv/en/branches_regions/index.shtml{/msg}">{msg desc=""}Branches and Regions{/msg}</a></li>
						<li><a href="{msg desc=""}http://iservice.prv/eng/index.shtml{/msg}">{msg desc=""}iService{/msg}</a></li>
						<li><a href="{msg desc=""}http://esdc.prv/en/work_tools/index.shtml{/msg}">{msg desc=""}Work Tools{/msg}</a></li>
						<li><a href="{msg desc=""}http://esdc.prv/en/quick-links/index.shtml{/msg}">{msg desc=""}Quick Links{/msg}</a></li>
					</ul>
				</div>
			</div>
		</nav>
		{elseif $subTheme == 'eccc'}
		//Todo: Change with eccc Menu
		<nav role="navigation" id="wb-sm" data-wb-ajax='{lb}"url": "{if isNonnull($menuPath)}{$menuPath}{else}{if $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{elseif $cdnEnv=='esdcprod'}https://templates.service.gc.ca{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_40/cdts/ajax/{msg desc=""}sitemenu-eng.html{/msg}{/if}", "type": "replace", "nocache": true {rb}' data-trgt="mb-pnl" class="wb-menu visible-md visible-lg" typeof="SiteNavigationElement">
			<div class="container nvbar">
				<h2>{msg desc=""}Topics menu{/msg}</h2>
				<div class="row">
					<ul class="list-inline menu">
						<li><a href="#">{msg desc="This menu item should be reviewed"}About ECCC{/msg}</a></li>
						<li><a href="#">{msg desc="This menu item should be reviewed"}Branches{/msg}</a></li>
						<li><a href="#">{msg desc="This menu item should be reviewed"}Services{/msg}</a></li>
						<li><a href="#">{msg desc="This menu item should be reviewed"}Tools and References{/msg}</a></li>
						<li><a href="#">{msg desc="This menu item should be reviewed"}Resources{/msg}</a></li>
						<li><a href="#">{msg desc="This menu item should be reviewed"}Forms{/msg}</a></li>
						<li><a href="#">{msg desc="This menu item should be reviewed"}DMs' Corner{/msg}</a></li>
					</ul>
				</div>
			</div>
		</nav>
		{else}
		<nav role="navigation" id="wb-sm" data-ajax-replace="{msg desc=""}https://ssl-templates.services.gc.ca/app/cls/WET/gcintranet/v4_0_40/cdts/ajax/sitemenu-eng.html{/msg}" data-trgt="mb-pnl" class="wb-menu visible-md visible-lg" typeof="SiteNavigationElement">
			<div class="container nvbar">
				<h2>{msg desc=""}Topics menu{/msg}</h2>
				<div class="row">
					<ul class="list-inline menu">
						<li><a href="{msg desc=""}https://intranet.canada.ca/psc-fsc/index-eng.asp{/msg}">{msg desc=""}News{/msg}</a></li>
						<li><a href="{msg desc=""}https://intranet.canada.ca/hr-rh/index-eng.asp{/msg}">{msg desc=""}Human resources{/msg}</a></li>
